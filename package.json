  "lint-staged": {
    "*.(js|ts|tsx|jsx)": [
      "prettier --write",
      "eslint --fix",
      "eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.2",
