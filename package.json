    "eslint-define-config": "^1.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "husky": "^8.0.1",
    "pnpm": "^7.13.4",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "unbuild": "^0.9.4",
    "vitest": "^0.24.3"
  },
  "packageManager": "pnpm@6.32.3"
